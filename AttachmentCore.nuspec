<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AttachmentCore</id>
    <version>0.0.1-alpha</version>
    <authors>MohammadReza Rahimi</authors>
    <owners>MohammadReza Rahimi</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/attachmentcore/attachment-core</projectUrl>
    <description>A  module that is in charge of attach file to entities by specified entity-name field-name and entity-id in a ASP.NET Core applications</description>
    <copyright>Copyright ©2019 MohammadReza Rahimi</copyright>
    <license type="expression">MIT</license>
    <iconUrl>http://github.com/contoso/UsefulStuff/nuget_icon.png</iconUrl>
    <repository url="https://github.com/attachmentcore/attachment-core" />
    <releaseNotes>Bug fixes and performance improvements</releaseNotes>
    <tags>ASP.NET Core Attachment Module</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.AspNetCore" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Mvc" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.StaticFiles" version="2.2.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <contentFiles>
    <file src="Attachment\bin\Release\netstandard2.0\*.*" target="lib\netcore" />
    <file src="AttachmentSystem.Component\dist\*.*" target="content\wwwroot\attachment-core" />
  </contentFiles>
</package>